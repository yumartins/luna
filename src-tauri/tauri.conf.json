{
  "app": {
    "windows": [
      {
        "title": "Luna",
        "width": 800,
        "height": 600,
        "hiddenTitle": true,
        "titleBarStyle": "Overlay"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "build": {
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist",
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build"
  },
  "bundle": {
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "active": true,
    "targets": "all"
  },
  "$schema": "https://schema.tauri.app/config/2",
  "version": "0.1.0",
  "identifier": "com.luna-app",
  "productName": "luna-app"
}
