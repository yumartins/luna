{
  "vcs": {
    "enabled": false,
    "clientKind": "git",
    "useIgnoreFile": false
  },
  "files": {
    "ignoreUnknown": false,
    "ignore": []
  },
  "linter": {
    "rules": {
      "style": {
        "useNodejsImportProtocol": "off"
      },
      "correctness": {
        "noUnusedImports": "error"
      },
      "recommended": true
    },
    "enabled": true
  },
  "$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
  "formatter": {
    "enabled": true,
    "indentStyle": "tab"
  },
  "organizeImports": {
    "enabled": true
  }
}
